{
    "name": "oracle-subgraph",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "auth": "graph auth https://api.thegraph.com/deploy/",
        "codegen": "graph codegen",
        "build": "npm run prepare:gnosis && graph build",
        "prepare:gnosis": "ts-node bin/build-subgraph.ts xdai && npm run codegen",
        "prepare:mainnet": "ts-node bin/build-subgraph.ts mainnet && npm run codegen",
        "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ graphprotocol/oralce-subgraph",
        "create-local": "graph create --node http://localhost:8020/ graphprotocol/oralce-subgraph",
        "remove-local": "graph remove --node http://localhost:8020/ graphprotocol/oralce-subgraph",
        "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 graphprotocol/oralce-subgraph",
        "test": "echo 'No tests yet, implement them'"
    },
    "devDependencies": {
        "@graphprotocol/graph-cli": "^0.37.1",
        "@graphprotocol/graph-ts": "^0.29.1",
        "@types/node": "^18.11.18",
        "ts-node": "^10.8.2",
        "yaml": "^2.2.1"
    }
}
