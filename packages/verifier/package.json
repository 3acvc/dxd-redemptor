{
    "name": "dxd-redemptor-verifier",
    "version": "1.0.0",
    "description": "DXD redemptor verifier service",
    "main": "build/index.js",
    "scripts": {
        "test": "jest --passWithNoTests",
        "build": "tsc",
        "dev": "cross-env DEBUG=*,-nodemon:*,-eslint:*,-eslintrc:*,-typescript-eslint:*,-nodemon nodemon -x node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts",
        "lint": "prettier --check . !./dist !./.turbo && eslint \"./**!(dist|.turbo)/*+(.tsx|.ts)\"",
        "format": "prettier --write . !./dist !./.turbo && eslint --fix \"./**!(dist|.turbo)/*+(.tsx|.ts)\"",
        "lint:fix": "npm run lint -- --fix",
        "graphql:codegen": "graphql-codegen --config graphql-codegen.yml",
        "typechain:codegen": "typechain --target ethers-v5 --out-dir src/modules/generated/contracts \"src/abis/*.json\"",
        "start": "node --experimental-specifier-resolution=node build/index.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@ethersproject/abstract-provider": "^5.7.0",
        "@ethersproject/address": "^5.7.0",
        "@ethersproject/providers": "^5.7.0",
        "@ethersproject/wallet": "^5.7.0",
        "@hapi/boom": "^10.0.0",
        "@hapi/hapi": "^20.2.2",
        "@hapi/inert": "^7.0.0",
        "@hapi/vision": "^7.0.0",
        "@sentry/node": "^7.3.0",
        "@sentry/tracing": "^7.7.0",
        "axios": "^0.27.2",
        "chalk": "^4.1.2",
        "dotenv": "^16.0.1",
        "dxd-redemptor-oracle": "workspace:*",
        "graphql-request": "^5.0.0",
        "graphql-tag": "^2.12.6",
        "hapi-rate-limit": "^6.0.0",
        "hapi-swagger": "^14.5.5",
        "joi": "^17.6.0",
        "mongoose": "^6.5.0"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@graphql-codegen/cli": "^2.6.2",
        "@graphql-codegen/typescript-graphql-request": "^4.4.10",
        "@graphql-codegen/typescript-operations": "^2.4.2",
        "@shelf/jest-mongodb": "^3.0.1",
        "@typechain/ethers-v5": "^10.1.1",
