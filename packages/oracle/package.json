{
    "name": "dxd-redemptor-oracle",
    "type": "module",
    "private": true,
    "version": "1.0.0",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs"
        }
    },
    "dependencies": {
        "@ethersproject/abi": "^5.7.0",
        "@ethersproject/abstract-provider": "^5.7.0",
        "@ethersproject/abstract-signer": "^5.7.0",
        "@ethersproject/address": "^5.7.0",
        "@swapr/sdk": "1.7.0-beta.0",
        "graphql-request": "^5.0.0",
        "graphql-tag": "^2.12.6"
    },
    "scripts": {
        "lint": "prettier --check . !./dist !./.turbo && eslint \"./**!(dist|.turbo)/*+(.tsx|.ts)\"",
        "format": "prettier --write . !./dist !./.turbo && eslint --fix \"./**!(dist|.turbo)/*+(.tsx|.ts)\"",
        "clean": "rimraf ./src/generated && rimraf ./dist",
        "build": "pnpm clean && pnpm codegen && rollup -c rollup.config.js",
        "test": "echo \"No tests, implement them\"",
        "graphql:codegen": "graphql-codegen --config graphql-codegen.yml",
        "typechain:codegen": "typechain --target ethers-v5 --out-dir ./src/generated/contracts \"./abis/*.json\" \"./node_modules/dxd-redemptor-contracts/abis/*.json\"",
        "codegen": "pnpm graphql:codegen && pnpm typechain:codegen"
    },
    "devDependencies": {
        "@ethersproject/providers": "^5.7.2",
        "@graphql-codegen/cli": "^2.6.2",
        "@graphql-codegen/typescript-graphql-request": "^4.4.10",
        "@graphql-codegen/typescript-operations": "^2.4.2",
        "@rollup/plugin-commonjs": "^23.0.2",
        "@rollup/plugin-json": "^5.0.1",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@typechain/ethers-v5": "^10.1.1",
        "@types/rollup-plugin-peer-deps-external": "^2.2.1",
        "dxd-redemptor-contracts": "workspace:*",
        "ethers": "^5.7.2",
        "rimraf": "^3.0.2",
        "rollup": "^3.2.3",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.34.0",
        "tsconfig": "workspace:*",
        "tslib": "^2.4.1",
        "typechain": "^8.1.1",
        "typescript": "^4.7.4"
    }
}
